{
  "manifest_version": 3,
  "name": "Auto Scroll Extension",
  "version": "1.0",
  "description": "Automatically scroll web pages",
  "permissions": ["activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}